{
    "name": "html-to-jsx",
    "version": "0.3.0-dev",
    "description": "Converts HTML to JSX for use with React",
    "main": "src/htmltojsx.js",
    "bin": "src/cli.js",
    "scripts": {
        "clean": "rimraf lib dist",
        "site": "webpack --progress --colors --config webpack.site.config.base.js",
        "dist": "webpack --progress --colors --config webpack.config.production.js",
        "dev": "webpack-dev-server --progress --color --port=8008 --hot --inline --config webpack.config.development.js"
    },
    "dependencies": {
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "yargs": "~4.6.0",
        "jsdom": "^9.9.0"
    },
    "devDependencies": {
        "babel-core": "^6.18.2",
        "babel-loader": "^6.2.8",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.5.0",
        "del": "~2.2.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "gulp": "~3.9.0",
        "gulp-babel": "^6.1.2",
        "gulp-gh-pages": "~0.5.4",
        "gulp-livereload": "~3.8.1",
        "gulp-rename": "~1.2.0",
        "gulp-uglify": "~1.5.1",
        "gulp-webpack": "~1.5.0",
        "html-loader": "^0.4.4",
        "merge-stream": "~1.0.0",
        "rimraf": "^2.5.4",
        "webpack": "~1.14.0",
        "webpack-dev-server": "^1.16.2"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/RequireSun/HTMLtoJSX.git"
    },
    "keywords": [
        "react",
        "jsx",
        "htmltojsx",
        "html",
        "react-component"
